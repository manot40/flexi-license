{
  "name": "flexi-license",
  "version": "1.0.0",
  "license": "proprietary",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "dotenv -e .env.development -- prisma db seed",
    "db:push": "dotenv -e .env.development -- prisma db push",
    "db:generate": "dotenv -e .env.development -- prisma generate",
    "db:seed:prod": "dotenv -e .env.production -- prisma db seed",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "db:generate:prod": "dotenv -e .env.production -- prisma generate",
    "prisma:studio": "dotenv -e .env.development -- prisma studio",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "format:write": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.8.0",
    "@mantine/form": "^5.8.0",
    "@mantine/hooks": "^5.8.0",
    "@mantine/next": "^5.8.0",
    "@mantine/notifications": "^5.8.0",
    "@mantine/nprogress": "^5.8.0",
    "@next/font": "^13.0.3",
    "@prisma/client": "^4.6.1",
    "@tabler/icons": "^1.112.0",
    "bcryptjs": "^2.4.3",
    "cookies-next": "^2.1.1",
    "dayjs": "^1.11.6",
    "jose": "^4.11.0",
    "ms": "^2.1.3",
    "next": "^13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^1.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/ms": "^0.7.31",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.6.1",
    "ts-node": "^10.9.1",
    "typescript": "4.9.3"
  },
  "engines": {
    "node": "^16.x"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}